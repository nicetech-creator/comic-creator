# File name: drawingspace.kv
<DrawingSpace@RelativeLayout>:
	canvas.before:
		Line:
			rectangle: 0, 0, self.width - 4,self.height - 4
	StickMan:
		pos_hint: {'center_x':.5,'center_y':.5}
		canvas.before:
			Translate:
				xy: -self.width/2, -self.height/2
			Scale:
				xyz: 4,4,0
	StickMan: